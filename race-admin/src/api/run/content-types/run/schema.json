{
  "kind": "collectionType",
  "collectionName": "runs",
  "info": {
    "singularName": "run",
    "pluralName": "runs",
    "displayName": "Run",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "chrono": {
      "type": "string",
      "regex": "^(?:\\d{2}:)*?[0-5]\\d:[0-2]\\d\\.\\d{3}$",
      "default": "00:00.000"
    },
    "numberSign": {
      "type": "integer",
      "min": 1,
      "max": 100
    },
    "walking": {
      "type": "boolean",
      "default": false
    },
    "copyright": {
      "type": "boolean",
      "default": true
    },
    "runner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::runner.runner",
      "inversedBy": "runs"
    },
    "dropped": {
      "type": "boolean",
      "default": false
    },
    "race": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::race.race",
      "inversedBy": "runs"
    }
  }
}
